Link : https://leetcode.com/problems/find-the-duplicate-number/description/

Code : 

class Solution {
public:
    int findDuplicate(vector<int>& nums) {
        unordered_map<int,int> mp;
        int ans;
        for(int i =0 ;i<nums.size();i++){
            mp[nums[i]]++;
        }
        for(auto i : mp){
            if(i.second >1) ans = i.first;
        }
        return ans;
    }
};
